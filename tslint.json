{
  "extends": "tslint:latest",
  "rules": {
    "interface-name": false,
    "interface-over-type-literal": false,
    "variable-name": {
      "options": [
        "allow-leading-underscore"
      ]
    }
  },
  "linterOptions": {
    "exclude": [
      "lib/graphql/schema.ts",
      "tests/**/*.spec.ts"
    ]
  }
}